                <!-- The surface tension of the cell membranes [M / T^2] <! -->
                <surface_tension>1e-3</surface_tension> 

                <!-- The bending elasticity of the cell surface [M L^2 / T^2] <! -->
                <bending_modulus>0</bending_modulus> 
            </face_type>
        </face_types>  
    </cell_type>

